extends layout

block content
    h1 Hello!
    if places.length
        p Here are the addresses:
        table
            thead
                td ID
                td Name
                td Coordinate
                td Type
            each place in places
                tr
                    td #{place.id}
                    td #{place.Name}
                    td #{place.Coordinate}
                    td #{place.Type}

        pre= JSON.stringify(people, null, 2)
    else
        p No places added yet!
    script.
        console.log(!{JSON.stringify(places)})
